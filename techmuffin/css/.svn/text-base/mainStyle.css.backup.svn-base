#wrapper{
    position: relative;
	width:80%;
	margin-left: Auto;
	margin-right: Auto;
	paddin: 1em;
}
#content {
	background-color: White;
	display:block;
	height:98%;
	position:fixed;
	width: 80%;
	top: 0.5%;
}

#titleBar{
	padding-top;5px;
	text-align:left;
	width:100%;
	height:156px;
	float:left;
}
#titleBar img{
	margin: 0;
    display:block;
    top: 0%;
    left: 0%;
    height: 156px;
 	z-index:-2;
}
#titleBar span{
	font-family: AR DELANEY;
	color: #499393;
	margin-left: 20px;
	margin-top: 10px;
	display: inline-block;
	padding: 0;	
}
#titleBar h1{
	font-size: 30px;
	margin: 0;
}
#titleBar h4{
	font-family: AR DECODE;
	font-size: 10px;
	margin: 0;
}

#workspace{
	height: 88%;
	width: 80%;
	margin-left: Auto;
	margin-right: Auto;
}
#footer{
	height: 100%;
}

body{
	background-color: LightBlue;
}