  		$('#socialRegistrationDiv').hide();
  		$('#socialRegistrationErrMsg').hide();
        $('#createEventDiv').hide();
        $('#getRegistrantByEmailDiv').hide();
        $('#workspaceTitle').text("");
        $('#eventSettingsDiv').hide();

  	}
  	$(document).ready(function(){
  		hideAll();
  	});

  	$('#getEventByIDLink').click(function(){
  	    hideAll();
  	    loadEventSettingsForm();
  	});
  	
  	$('#socialRegistrationLink').click(function(){
  		hideAll();
  		loadSocialRegistrationView();
  	});
  	
  	$('#addRegistrantLink').click(function(){
  		hideAll();
  		loadAddRegistrantView();
  	});
  	
  	$('#getRegistrantByEmailLink').click(function(){
  		hideAll();
  		loadGetRegistrantByEmailView();
  	});
  	
  	$('#getRegistrantByIDLink').click(function(){
  		hideAll();
  		loadGetRegistrantByIDView();
  	})

    $('#createEventLink').click(function(){
        hideAll();
        loadCreateEventView();
    })


