  <script type="text/javascript">
  	function hideAll(){
  		$('#addRegistrantDiv').hide();
  		$('#addRegistrantSuccessMsg').hide();
  		$('#addRegistrantErrMsg').hide();
  		$('#errorMsgContainer').hide();
        $('#getRegistrantByIDDiv').hide();
