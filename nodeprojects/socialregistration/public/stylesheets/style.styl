a
  color: #00B7FF

.error
  color: red
  font: 25px "Lucida Grande", Helvetica, Arial, sans-serif
 
body
  margin:0;
  padding:0;
  line-height: 1.5em;
  background-color: #FFFFFC;
  text-align:center;

b
  font-size: 110%;

em
  color: red;

.hidden
  display:none;

#titlePanel
  padding-top;2px;
  text-align:left;
  width:100%;
  float:left;

#titlePanel p
  margin: 5px;

#titlePanel img
  margin: 0;
  display:block;
  top: 0%;
  left: 0%;
  height: 156px;
  z-index:-2;
  float: left;
  clear: right;
  margin-right: 2em;

#titlePanel span
  font-family: AR DELANEY;
  color: #499393;
  margin-left: 20px;
  margin-top: 10px;
  display: inline-block;
  padding: 0;  
  width: 100%;

#titlePanel h1
  font-size: 25px;
  margin: 0;
  margin-top: 1em;

#titlePanel h4
  font-family: AR DECODE;
  font-size: 15px;
  margin: 0;

#menuPanel
  float:left;
  height: 2%;
  display: block;
  width: 100%;
  top: 12%;

#centerPanel
  width:95%;
  height:90%;
  padding:0;
  margin:auto;
  border: 0;
  display: block;

.sidebar
  min-height:450px;
  height: 100%;
  width: 30%;
  float: left;
  clear: right;
  border: 1px solid red;


#workspace
  text-align:left;
  float:left;
  min-height:450px;
  height: auto !important;
  width: 100%;
  border: 1px solid green
  
#workspace img
  margin-left: 3em;
  margin-top: 2em;  


#footerPanel
  width:100%;
  height:10%;
  float:left;
  bottom: 2%;


#mainNav
  display:block;
  float: right;
  width: 50%;

#mainNav li 
  padding:0 0 0 15px;
  margin-left: 0px; 
  list-style-position : outside;
  list-style-type: none;

#mainNav ul
  margin-top:2px;
  margin-left:1em;
  padding-left:0px;
  padding-right:5px;
  padding-bottom:0px;
  margin-bottom:0px;

#mainNav ul, #mainNav li 
  float:right;
  line-height:31px;

#mainNav a 
  color: #3F4445;
  font-weight:normal;
  text-decoration:none;
  cursor:pointer;
  border-bottom: 1px solid #277777;

#mainNav a:hover
  text-decoration: underline;


#paragraph 
  margin-bottom:10px;
  position:relative;
  text-align:justify;
  width:100%;

#header 
  font-size:11px;
  font-weight:bold;

#lineheader 
  font-size:16px;
  font-weight:bold;
  margin-bottom:10px;

body, div, p, td, th, select, input 
  font-family:Geneva,Arial,Helvetica,sans-serif,sans-serif;
  font-size:12px; 
  color: #3F4445; 